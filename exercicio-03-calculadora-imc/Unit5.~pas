{$R *.dfm}

procedure clickDinamico(Sender: TObject);
begin
  if TButton(Sender).Caption = '1') then
    ShowMessage('1');
  if (TButton(Sender).Caption = '2') then
    ShowMessage('2');
end;

procedure TfrmCalculadoraDinamica.FormShow(Sender: TObject);
var
  x, y, i, j: Integer;
  b: TButton;
begin
  x:= 5;
  y:= 5;
  j:= 1;

  {for i:= 0 to 9 do
    b:= TB}


end;

end.
